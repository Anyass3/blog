{
  "name": "blog",
  "version": "0.1.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "dmt-integrate": "chmod +x dmt/dmt-integrate && ./dmt/dmt-integrate",
    "lint:fix": "yarn format . && eslint . --fix"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.1",
    "@sveltejs/adapter-node": "^5.2.0",
    "@sveltejs/kit": "^2.5.18",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tailwindcss/typography": "^0.5.13",
    "@types/cookie": "^0.5.1",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "autoprefixer": "10.4.19",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.41.0",
    "globals": "^15.8.0",
    "postcss": "8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.5",
    "svelte": "^5.0.0-next.181",
    "svelte-check": "^3.8.4",
    "svelte-feather-icons": "4.1.0",
    "tailwindcss": "3.4.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0-alpha.20",
    "vite": "^5.3.3",
    "vitest": "^1.6.0"
  },
  "type": "module",
  "dependencies": {
    "@anyass3/encryption": "github:Anyass3/Encryption",
    "@lukeed/uuid": "^2.0.0",
    "cookie": "^0.4.2",
    "dmt-gui-kit": "github:Anyass3/dmt-gui-kit",
    "hyperbee": "^2.2.0",
    "hypercore": "^10.5.3",
    "liquidjs": "^10.4.0",
    "markdown-hljs": "^0.1.11",
    "nodemailer": "^6.9.14",
    "random-access-memory": "^6.1.0",
    "stores-x": "^0.0.872",
    "svelte-email": "^0.0.4"
